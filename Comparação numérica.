#include<iostream>
#include<stdio.h>
#include<stdlib.h>

using namespace std;

int main(){

    double numero, numero1;

    cin >> numero;
    cin >> numero1;

    if(numero>numero1){
        printf("Digite um numero:\n");
        printf("Digite outro numero:\n");
        printf("Maior numero: %.1lf", numero);
    }
    else if(numero1 > numero){
        printf("Digite um numero:\n");
        printf("Digite outro numero:\n");
        printf("Maior numero: %.1lf", numero1);
    }

    return 0;

}

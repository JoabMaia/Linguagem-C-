#include<stdio.h>
#include<iostream>
#include<string.h>

using namespace std;

int main(){

    int valores[3], valor, i, maior;

    for(i=0; i<3; i++){
        cin >> valores[i];
    }
    cin >> valor;
    maior = valores[0];
    for(i=0; i<3; i++){
        if (valores[i] > maior){
            maior = valores[i];
        }
    }
    cout << int(maior * valor);
    return 0;
}

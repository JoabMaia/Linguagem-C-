#include<iostream>
#include<stdio.h>
#include<stdlib.h>

using namespace std;

int main(){
	float valores[4];
	int i, cont=0;
	
	for(i=0; i<=4; i++){
		cin >> valores[i];
	}
	for(i=0; i<=4; i++){
		if(valores[i] < 0){
			cont++;
		}
	}
	cout << "\nDigite um valor:";
	cout << "\nDigite um valor:";
	cout << "\nDigite um valor:";
	cout << "\nDigite um valor:";
	cout << "\nDigite um valor:\n";
	cout << "Foram digitados " << cont << " numeros negativos";
	return 0;
}
